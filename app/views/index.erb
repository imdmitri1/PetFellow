<a href="/users"> users </a>

<p><strong><a href="/">recently added</a></strong><strong><a href="/?popular">popular</a></strong></p>

<div>
  <% @posts.each do |post| %>
  <div>
    <a href="/posts/<%=post.id%>">
      <img src="<%=post.pic_link%> " alt="<%=post.pic_link%> ">
      <div><%=post.description%> <br> <%=post.created_at%> </div>
    </a>
  </div>
  <% end %>
</div>

<div class="pagination">
  <%= will_paginate @posts %>
</div>
